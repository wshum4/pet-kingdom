<%= simple_form_for(request) do |f| %>
  <%= f.input :service, :collection => ['Grooming', 'Walking', 'Sitting', 'Cuddling'] %>
  <%= f.input :animal, collection: Animal.all %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :animal_info%>
  <%= f.input :housing %>
  <%= f.input :message %>
  <%= f.submit %>
<% end %>
